{
  "compilerOptions": {
    "noImplicitAny": true,
    "lib": ["DOM", "ESNext"],
    "target": "ES5",
    "jsx": "react-jsx",
    "moduleResolution": "nodenext",
    "strict": true,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "typeRoots": ["./types", "node_modules/@types"]
  }
}
